    <!-- https://vocab.um.es#hasCompany -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasCompany">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="https://vocab.um.es#Company"/>
        <dc:creator xml:lang="en">OpenAI Ontology Team</dc:creator>
        <dc:date xml:lang="en">2023-10-11</dc:date>
        <rdfs:comment xml:lang="en">This property links a customer complaint to the company related to the complaint.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <rdfs:label xml:lang="en">Associated Business</rdfs:label>
        <rdfs:label xml:lang="en">Company Associated</rdfs:label>
        <rdfs:label xml:lang="en">Company in Complaint</rdfs:label>
        <rdfs:label xml:lang="en">Complaint&apos;s Company</rdfs:label>
        <rdfs:label xml:lang="en">Related Company</rdfs:label>
        <owl:versionInfo xml:lang="en">Version 1.0</owl:versionInfo>
        <skos:altLabel xml:lang="en">Concerned Company</skos:altLabel>
        <skos:definition xml:lang="en">An object property that associates a customer complaint with the concerned company.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasComplaint -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasComplaint">
        <rdfs:domain rdf:resource="https://vocab.um.es#Product"/>
        <rdfs:range rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:comment xml:lang="en">This property relates a product to a customer complaint regarding that product.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <rdfs:label xml:lang="en">has complaint</rdfs:label>
        <owl:versionInfo xml:lang="en">Version 1.0</owl:versionInfo>
        <skos:altLabel xml:lang="en">associated with complaint</skos:altLabel>
        <skos:altLabel xml:lang="en">has feedback</skos:altLabel>
        <skos:altLabel xml:lang="en">is related to complaint</skos:altLabel>
        <skos:altLabel xml:lang="en">is subject of complaint</skos:altLabel>
        <skos:altLabel xml:lang="en">receives complaint for</skos:altLabel>
        <skos:definition xml:lang="en">A property that links a product to a customer complaint pertaining to it.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasProblem -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasProblem">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
        <rdfs:comment xml:lang="en">This property links a customer complaint to the specific problem or issue being reported.</rdfs:comment>
        <rdfs:description xml:lang="en">The specific issue or concern described in a customer complaint.</rdfs:description>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <rdfs:label xml:lang="en">Has Problem</rdfs:label>
        <altLabel>AssociatedProblem</altLabel>
        <altLabel>ComplaintConcern</altLabel>
        <altLabel>ComplaintIssue</altLabel>
        <altLabel>MainProblem</altLabel>
        <altLabel>ReportedIssue</altLabel>
        <alternativeName>ProblemAssociated</alternativeName>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasProduct -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasProduct">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="https://vocab.um.es#Product"/>
        <dc:creator xml:lang="en">Ontology Engineering Team</dc:creator>
        <rdfs:comment xml:lang="en">This property links a customer complaint to the related product.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <rdfs:label xml:lang="en">associatedProduct</rdfs:label>
        <rdfs:label xml:lang="en">complaintForProduct</rdfs:label>
        <rdfs:label xml:lang="en">hasProduct</rdfs:label>
        <rdfs:label xml:lang="en">linkedProduct</rdfs:label>
        <rdfs:label xml:lang="en">productOfComplaint</rdfs:label>
        <rdfs:label xml:lang="en">relatedProduct</rdfs:label>
        <owl:versionInfo xml:lang="en">Version 1.0</owl:versionInfo>
        <skos:altLabel xml:lang="en">Product in Issue</skos:altLabel>
        <skos:definition xml:lang="en">A property that establishes a relationship between a customer complaint and the product that the complaint pertains to.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasResolution -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasResolution">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="https://vocab.um.es#Resolution"/>
        <dc:creator xml:lang="en">ChatGPT Ontology Team</dc:creator>
        <dc:date xml:lang="en">2023-10-11</dc:date>
        <rdfs:comment xml:lang="en">This property links a company&apos;s response to the resolution associated with it.</rdfs:comment>
        <rdfs:label xml:lang="en">Contains Resolution</rdfs:label>
        <rdfs:label xml:lang="en">Features Resolution</rdfs:label>
        <rdfs:label xml:lang="en">Has Resolution</rdfs:label>
        <rdfs:label xml:lang="en">Holds Resolution</rdfs:label>
        <rdfs:label xml:lang="en">Possesses Resolution</rdfs:label>
        <owl:versionInfo xml:lang="en">Version 1.1</owl:versionInfo>
        <skos:altLabel>Resolution Relation</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasResponse -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasResponse">
        <rdfs:domain rdf:resource="https://vocab.um.es#Company"/>
        <rdfs:range rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <dc:creator xml:lang="en">Ontology Engineering Team - UM</dc:creator>
        <dc:description xml:lang="en">A property that connects a company to its specific response related to a complaint or inquiry. It is vital for tracking company feedback or reactions to customer issues.</dc:description>
        <rdfs:comment>This property associates a company with its respective response to customer complaints or inquiries.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <owl:versionInfo>Version 1.1</owl:versionInfo>
        <skos:altLabel xml:lang="en">companyFeedback</skos:altLabel>
        <skos:altLabel xml:lang="en">companyReaction</skos:altLabel>
        <skos:altLabel xml:lang="en">companyReply</skos:altLabel>
        <skos:altLabel xml:lang="en">responseByCompany</skos:altLabel>
        <skos:altLabel xml:lang="en">responseFromCompany</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasState -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasState">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="https://vocab.um.es#State"/>
        <dc:creator xml:lang="en">OpenAI Ontology Team</dc:creator>
        <dc:date xml:lang="en">2023-10-11</dc:date>
        <rdfs:comment xml:lang="en">Property indicating the state or status of a particular customer complaint.</rdfs:comment>
        <rdfs:description xml:lang="en">This property represents the current condition, phase, or status that a complaint resides in.</rdfs:description>
        <rdfs:label xml:lang="en">State of Complaint</rdfs:label>
        <skos:altLabel xml:lang="en">ComplaintCondition</skos:altLabel>
        <skos:altLabel xml:lang="en">ComplaintPhase</skos:altLabel>
        <skos:altLabel xml:lang="en">ComplaintStatus</skos:altLabel>
        <skos:altLabel xml:lang="en">CurrentStateOfComplaint</skos:altLabel>
        <skos:altLabel xml:lang="en">IssueState</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasSubProblem -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasSubProblem">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
        <rdfs:range rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <dc:creator xml:lang="en">OpenAI Ontology Team</dc:creator>
        <dc:date>2023-10-11</dc:date>
        <dc:description xml:lang="en">A property indicating the relationship between a general problem and its more specific sub-problem.</dc:description>
        <rdfs:comment xml:lang="en">This property links a specific problem of complaint to its related sub-category of problems.</rdfs:comment>
        <rdfs:label xml:lang="en">Associated Sub-Problem</rdfs:label>
        <rdfs:label xml:lang="en">Contains Sub-Problem</rdfs:label>
        <rdfs:label xml:lang="en">Has Sub-Problem</rdfs:label>
        <rdfs:label xml:lang="en">Linked Sub-Problem</rdfs:label>
        <rdfs:label xml:lang="en">Related Sub-Problem</rdfs:label>
        <owl:alternativeName>RelatesSubProblemTo</owl:alternativeName>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#hasSubProduct -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasSubProduct">
        <rdfs:domain rdf:resource="https://vocab.um.es#Product"/>
        <rdfs:range rdf:resource="https://vocab.um.es#SubProduct"/>
        <dc:creator xml:lang="en">OpenAI Ontology Engineering Team</dc:creator>
        <dc:date>2023-10-11</dc:date>
        <dc:description xml:lang="en">A relation indicating that a product has a specific sub-product or variant.</dc:description>
        <rdfs:comment xml:lang="en">This property links a product to its specific sub-products or variants.</rdfs:comment>
        <rdfs:label xml:lang="en">Associated with Sub-Product</rdfs:label>
        <rdfs:label xml:lang="en">Comprises Sub-Product</rdfs:label>
        <rdfs:label xml:lang="en">Contains Sub-Product</rdfs:label>
        <rdfs:label xml:lang="en">Has Sub-Product</rdfs:label>
        <rdfs:label xml:lang="en">Incorporates Sub-Product</rdfs:label>
        <owl:alternativeName>Links to Sub-Product</owl:alternativeName>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#relatedToComplaint -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#relatedToComplaint">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="https://vocab.um.es#CustomerComplaint"/>
    </owl:ObjectProperty>
    


    <!-- https://vocab.um.es#underProblem -->

    <owl:ObjectProperty rdf:about="https://vocab.um.es#underProblem">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
    </owl:ObjectProperty>
    


