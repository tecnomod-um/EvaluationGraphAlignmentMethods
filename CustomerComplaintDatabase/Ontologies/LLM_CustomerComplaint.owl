        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#dateReceived -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#dateReceived">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#description -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#description">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:domain rdf:resource="https://vocab.um.es#Resolution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#narrative -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#narrative">
        <rdfs:domain rdf:resource="https://vocab.um.es#CustomerComplaint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#productName -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#productName">
        <rdfs:domain rdf:resource="https://vocab.um.es#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name of the product.</rdfs:comment>
        <rdfs:description xml:lang="en">The official name given to a product.</rdfs:description>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
        <rdfs:label xml:lang="en">Product Name</rdfs:label>
        <owl:versionInfo>Version 1.0</owl:versionInfo>
        <alternativeName>productTitle</alternativeName>
        <label xml:lang="en">Item Name</label>
        <label xml:lang="en">Product Designation</label>
        <label xml:lang="en">Product Identifier</label>
        <label xml:lang="en">Product Title</label>
        <label xml:lang="en">ProductName</label>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#response -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#response">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dc:creator xml:lang="en">Ontology Design Team at UM</dc:creator>
        <dc:date xml:lang="en">2023-10-11</dc:date>
        <rdfs:comment xml:lang="en">This property represents the textual response provided by a company to a customer complaint.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <skos:altLabel xml:lang="en">business_response</skos:altLabel>
        <skos:altLabel xml:lang="en">company_reply</skos:altLabel>
        <skos:altLabel xml:lang="en">corporate_feedback</skos:altLabel>
        <skos:altLabel xml:lang="en">official_answer</skos:altLabel>
        <skos:altLabel xml:lang="en">organization_rejoinder</skos:altLabel>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#subProductDescription -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#subProductDescription">
        <rdfs:domain rdf:resource="https://vocab.um.es#SubProduct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://vocab.um.es#timelyResponse -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#timelyResponse">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <dct:description xml:lang="en">A datatype property that represents if the company&apos;s response to a complaint was delivered in a timely manner.</dct:description>
        <rdfs:comment xml:lang="en">Indicates whether the company&apos;s response was timely or not.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://vocab.um.es"/>
        <rdfs:label xml:lang="de">Rechtzeitige Antwort</rdfs:label>
        <rdfs:label xml:lang="es">Respuesta a Tiempo</rdfs:label>
        <rdfs:label xml:lang="it">Risposta Tempestiva</rdfs:label>
        <rdfs:label xml:lang="fr">Réponse en Temps</rdfs:label>
        <rdfs:label xml:lang="en">Timely Response</rdfs:label>
        <owl:versionInfo>1.1</owl:versionInfo>
        <skos:altLabel xml:lang="en">Prompt Response</skos:altLabel>
        <skos:prefLabel xml:lang="en">On-Time Response</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- 
