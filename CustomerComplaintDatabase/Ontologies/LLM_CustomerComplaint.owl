    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://vocab.um.es#Company -->

    <owl:Class rdf:about="https://vocab.um.es#Company">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCompany"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#CustomerComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://vocab.um.es#CompanyResponse -->

    <owl:Class rdf:about="https://vocab.um.es#CompanyResponse">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasResolution"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#response"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#timelyResponse"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://vocab.um.es#CustomerComplaint -->

    <owl:Class rdf:about="https://vocab.um.es#CustomerComplaint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCompany"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProblem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasState"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#dateReceived"/>
                <owl:hasValue rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#narrative"/>
                <owl:hasValue rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://vocab.um.es#ProblemOfComplaint -->

    <owl:Class rdf:about="https://vocab.um.es#ProblemOfComplaint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasSubProblem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProblem"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://vocab.um.es#CustomerComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://vocab.um.es#ProblemSubCategory -->

    <owl:Class rdf:about="https://vocab.um.es#ProblemSubCategory">
